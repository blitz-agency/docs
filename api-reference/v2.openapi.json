		"/v2/enrichment/linkedin-to-domain": {
			"post": {
				"summary": "Linkedin URL to Domain",
				"description": "### ðŸ’° Cost: 0.5 Credits (on success)\n\nExtract the primary verified email domain pattern associated with a Company LinkedIn URL.",
				"tags": ["Company Enrichment"],
				"security": [{ "APIKey": [] }],
				"requestBody": {
